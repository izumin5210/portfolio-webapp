{
  "name": "portfolio-webapp",
  "private": true,
  "workspaces": [
    "./packages/*"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "frolint --branch origin/main --expect-no-errors --expect-no-diff",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf src/**/__generated__/**/*",
    "graphql:dump-schema": "SCHEMA_ENDPOINT=http://localhost:3000/api/graphql graphql-codegen",
    "generate": "run-p 'generate:*'",
    "generate:next-typed-routes": "node ./scripts/gen-typed-routes.mjs",
    "generate:relay": "relay-compiler --schema schema.graphql --src ./src/ ",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "g:tsc": "cd $INIT_CWD && tsc",
    "g:jest": "cd $INIT_CWD && jest",
    "bootstrap": "yarn workspaces foreach --topological --parallel run prepack"
  },
  "dependencies": {
    "@linaria/react": "^3.0.0-beta.15",
    "@sentry/nextjs": "^6.17.9",
    "@types/js-yaml": "^4.0.5",
    "@types/react-relay": "^13.0.0",
    "@types/relay-runtime": "^13.0.1",
    "feed": "^4.2.2",
    "graphql": "^16.3.0",
    "graphql-helix": "^1.11.0",
    "graphql-relay": "^0.10.0",
    "graphql-scalars": "^1.14.1",
    "js-yaml": "^4.1.0",
    "next": "12.1.0",
    "node-fetch": "^3.2.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-relay": "^13.0.1",
    "rehype-prism-plus": "^1.3.1",
    "rehype-react": "^7.0.4",
    "rehype-stringify": "^9.0.3",
    "relay-runtime": "^13.0.1",
    "remark-extract-lead": "workspace:^",
    "remark-frontmatter": "4.0.1",
    "remark-h1-as-title": "workspace:^",
    "remark-meta-description": "workspace:^",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "sanitize.css": "^13.0.0",
    "unified": "^10.1.1"
  },
  "devDependencies": {
    "@babel/core": "7.17.5",
    "@graphql-codegen/cli": "2.4.0",
    "@graphql-codegen/schema-ast": "2.4.1",
    "@linaria/babel-preset": "3.0.0-beta.15",
    "@linaria/core": "3.0.0-beta.15",
    "@linaria/shaker": "3.0.0-beta.15",
    "@linaria/webpack-loader": "3.0.0-beta.16",
    "@storybook/addon-actions": "6.4.17",
    "@storybook/addon-essentials": "6.4.17",
    "@storybook/addon-links": "6.4.17",
    "@storybook/react": "6.4.17",
    "@testing-library/jest-dom": "5.16.1",
    "@testing-library/react": "12.1.2",
    "@types/glob": "7.2.0",
    "@types/node": "17.0.0",
    "@types/open-graph-scraper": "4.8.1",
    "@types/react": "17.0.39",
    "@types/relay-test-utils": "6.0.5",
    "babel-loader": "8.2.3",
    "babel-plugin-macros": "3.1.0",
    "babel-plugin-relay": "13.0.1",
    "eslint": "7.32.0",
    "eslint-config-next": "12.1.0",
    "eslint-plugin-relay": "1.8.3",
    "frolint": "2.8.2",
    "glob": "7.2.0",
    "jest": "27.4.7",
    "next-linaria": "1.0.1-beta",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "react-test-renderer": "17.0.2",
    "relay-compiler": "13.0.1",
    "relay-test-utils": "13.0.1",
    "rimraf": "3.0.2",
    "scripts": "workspace:^",
    "typescript": "4.5.5"
  },
  "packageManager": "yarn@3.1.1"
}
